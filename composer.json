{
    "name": "guillaume/shop-generator",
    "license": "proprietary",
    "type": "project",
    "description": "Project for Ryuutama shop-generation",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sensio/generator-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "symfony/assetic-bundle": "^2.7",
        "egeloen/ckeditor-bundle": "^4.0",
        "friendsofsymfony/user-bundle": "dev-master",
        "symfony/property-access": "^3.0",
        "vich/uploader-bundle": "@dev",
        "stof/doctrine-extensions-bundle": "^1.2",
        "laupifrpar/pusher-bundle": "^1.2",
        "coresphere/console-bundle": "dev-master",
        "pinano/select2-bundle": "dev-master",
        "leafo/scssphp": "^0.6.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^2.7"
    },
    "scripts": {
        "warmup": [
            "ItemBundle\\Composer\\HerokuEnvironment::populateEnvironment",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "bin/console doctrine:migrations:migrate",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "pre-install-cmd": [
            "ItemBundle\\Composer\\HerokuEnvironment::populateEnvironment",
            "mv web/app_dev.php web/943489347721.php"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database.name": "SYMFONY__DATABASE__NAME",
                "database.user": "SYMFONY__DATABASE__USER",
                "database.host": "SYMFONY__DATABASE__HOST",
                "database.password": "SYMFONY__DATABASE__PASSWORD",
                "mailer.user": "SYMFONY__SENDGRID__USERNAME",
                "mailer.password": "SYMFONY__SENDGRID__PASSWORD",
                "mailer.port": "SYMFONY__SENDGRID__PORT",
                "mailer.host": "SYMFONY__SENDGRID__HOST",
                "pusher.app_id": "SYMFONY__PUSHER__APPID",
                "pusher.key": "SYMFONY__PUSHER__KEY",
                "pusher.secret": "SYMFONY__PUSHER__SECRET",
                "mailer.transport": "smtp"
            }
        }
    }
}
